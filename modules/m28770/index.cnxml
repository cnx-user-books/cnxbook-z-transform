                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {}=1+ \( 0 "." 8z rSup { size 8{ - 1} }  \) + \( 0 "." 8z rSup { size 8{ - 1} }  \)  rSup { size 8{2} } + \( 0 "." 8z rSup { size 8{ - 1} }  \)  rSup { size 8{3} } + "."  "."  "." } {}</m:annotation>
