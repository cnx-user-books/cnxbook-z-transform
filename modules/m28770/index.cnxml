                      <m:msup>
                        <m:mn>2z</m:mn>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:mrow>
                              <m:mo stretchy="false">−</m:mo>
                              <m:mn>1</m:mn>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                      </m:msup>
                      <m:mrow>
                        <m:mrow>
                          <m:msup>
                            <m:mo stretchy="false">)</m:mo>
